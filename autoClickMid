var c = document.getElementsByTagName('input');
for (let index = 0; index < c.length; index++) {
    if (c[index].type === 'radio') {
	    if(c[index].value == '1') {
            c[index].click();
        }
    }
}
c["313_5"].click();
c["314_5"].click();
c["301_1"].click();
c["301_2"].click();
c["308_1"].click();
c["315_1"].click();
c["315_2"].click();
c["315_3"].click();
c["315_4"].click();

